{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "org-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/org/OrgWebPart.js",
          "manifest": "./src/webparts/org/OrgWebPart.manifest.json"
        }
      ]
    },
    "org-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/org/OrgApplicationCustomizer.js",
          "manifest": "./src/extensions/org/OrgApplicationCustomizer.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "OrgWebPartStrings": "lib/webparts/org/loc/{locale}.js",
    "OrgApplicationCustomizerStrings": "lib/extensions/org/loc/{locale}.js",
    "PropertyControlStrings": "node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js",
    "ControlStrings": "node_modules/@pnp/spfx-controls-react/lib/loc/{locale}.js"
  }
}